#!/usr/bin/env node
'use strict';

const { Environment } = require('../dist/commonjs/src');

let { plugins } = Environment.create();
let message = plugins.map((p) => {
  return `Name: ${p.name} Levels: ${p.modes.join(', ')}`
}).join('\n');

message = `\nPlugins\n========\n${message}`;

console.log(`${message}`);